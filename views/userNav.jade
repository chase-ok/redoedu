#log-in-window(title="Log In")
    p.validate 

    form.form-inline(role='form')
        fieldset
            .form-row
                label(for="email") Email
                input.ui-widget-content.ui-corner-all#email(type="text",name="email",style="width:200px")
            .form-row
                label(for="password") Password
                input.ui-widget-content.ui-corner-all#password(type="password",name="password",style="width:200px")
